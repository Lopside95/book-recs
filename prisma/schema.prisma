datasource db {
    provider = "mongodb"
    url      = env("MONGODB_URI")
}

generator client {
    provider = "prisma-client-js"
}

model Book {
    id     String @id @default(auto()) @map("_id") @db.ObjectId
    genre  String
    author String
    rating Int
    title  String
}

model Testing {
    id       String @id @default(auto()) @map("_id") @db.ObjectId
    question String
    answer   String
}

model Yes {
    id   String @id @default(auto()) @map("_id") @db.ObjectId
    what String
}
